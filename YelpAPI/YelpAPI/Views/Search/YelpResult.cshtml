@model YelpAPI.Models.YelpSearchResult

@{

ViewData["Title"] = "YelpResult";

}

<h1>Search Result</h1>
<h2>Displaying @Model.businesses.Count results of @Html.ValueFor(model => model.total)</h2>


 @foreach (var items in Model.businesses)
 {
    <div>
        @Html.DisplayFor(modelitem => items.name)
    </div>

 }
  
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
